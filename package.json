{
	"name": "vinyl-rollup",
	"version": "0.6.1",
	"description": "A wrapper for rollup that produces a stream of Vinyl objects.",
	"scripts": {
		"release": "standard-version --sign",
		"test": "xo && nyc ava"
	},
	"engines": {
		"node": ">=8"
	},
	"author": "Corey Farrell",
	"license": "MIT",
	"keywords": [
		"gulp",
		"gulpplugin",
		"vinyl",
		"rollup",
		"source maps",
		"sourcemaps"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cfware/vinyl-rollup.git"
	},
	"bugs": {
		"url": "https://github.com/cfware/vinyl-rollup/issues"
	},
	"homepage": "https://github.com/cfware/vinyl-rollup#readme",
	"dependencies": {
		"gulp": "^4.0.2",
		"merge2": "^1.2.3",
		"p-map": "^3.0.0",
		"rollup": "^1.16.6",
		"vinyl": "^2.2.0",
		"vinyl-fs": "^3.0.3"
	},
	"devDependencies": {
		"@cfware/eslint-config-node": "^0.1.0",
		"@cfware/nyc": "^0.5.0",
		"ava": "^2.1.0",
		"babel-plugin-bare-import-rewrite": "^1.5.1",
		"concat-stream": "^2.0.0",
		"esm": "^3.2.25",
		"gulp-sourcemaps": "^2.6.5",
		"nyc": "^14.1.1",
		"p-event": "^4.1.0",
		"pump": "^3.0.0",
		"rollup-plugin-babel": "^4.3.3",
		"standard-version": "^7.0.0",
		"xo": "^0.24.0"
	},
	"xo": {
		"extends": "@cfware/eslint-config-node",
		"overrides": [
			{
				"files": "fixtures/*.js",
				"rules": {
					"import/no-extraneous-dependencies": "off",
					"import/no-unresolved": "off"
				}
			}
		]
	}
}
